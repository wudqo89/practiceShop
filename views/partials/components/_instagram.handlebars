<div class="instagram-title-wrap">
  <div class="row">
    <div class="col s12 m12 l12">
      <div class="bg-entire-wrap">
        <div class="bg-entire bg-instagram">
          <div class="instagram-text-wrap">
            <h2 class="second-title">#INSTAGRAM</h2>
            <p class="instagram-description">레이티드그린의 공식 인스타그램 계정.</p>
            <hr class="second-title-hr">
          </div>
        </div>
      </div>
      {{!-- end: bg-entire-wrap --}}
    </div>
    {{!-- end: col --}}
  </div>
  {{!-- end: row --}}
</div>
{{!-- end: instagram-title-wrap --}}
<div class="container">
  <div class="inner-container">
    <div class="row">
      <div class="col s6 m4 l4">
        {{> components/_instagramImg}}
      </div>
      <div class="col s6 m4 l4">
        {{> components/_instagramImg}}
      </div>
      <div class="col s6 m4 l4">
        {{> components/_instagramImg}}
      </div>
    </div>
    <div class="row">
      <div class="col s6 m4 l4">
        {{> components/_instagramImg}}
      </div>
      <div class="col s6 m4 l4">
        {{> components/_instagramImg}}
      </div>
      <div class="col s6 m4 l4">
        {{> components/_instagramImg}}
      </div>
    </div>
    {{!-- 더보기 --}}
    <div class="more" style="display: none;">
      <div class="row">
        <div class="col s6 m4 l4">
          {{> components/_instagramImg}}
        </div>
        <div class="col s6 m4 l4">
          {{> components/_instagramImg}}
        </div>
        <div class="col s6 m4 l4">
          {{> components/_instagramImg}}
        </div>
      </div>
    </div>
    {{!-- button --}}
    <div class="row">
      <div class="col s12 m12 l12">
        <div class="more-btn-wrap">
          <button class="btn btn-large #e0e0e0 grey lighten-2 black-text js-moreBtn">더보기</button>
        </div>
      </div>
    </div>
  </div>
  {{!-- end: inner-container --}}
</div>

<script>
  const moreEl = document.querySelector('.more');
  const moreBtn = document.querySelector('.js-moreBtn');
  moreBtn.addEventListener('click', () => {
    moreEl.style.display = "block";
  })

</script>